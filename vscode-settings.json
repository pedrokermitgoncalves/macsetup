{
  "color-highlight.markerType": "dot-before",
  "editor.fontFamily": "Fira Code, Menlo, Monaco, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 13,
  "editor.fontWeight": "500",
  "editor.formatOnSave": true,
  "editor.formatOnSaveTimeout": 1000,
  "editor.minimap.enabled": false,
  "editor.tabSize": 2,
  "explorer.confirmDelete": false,
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 500,
  "git.autofetch": true,
  "npm.packageManager": "yarn",
  "terminal.external.osxExec": "iterm.app",
  "terminal.integrated.fontFamily": "Fira Code, Menlo, Monaco, 'Courier New', monospace",
  "terminal.integrated.fontWeight": "bold",
  "terminal.integrated.fontWeightBold": "900",
  "terminal.integrated.shell.osx": "/bin/zsh",
  "tslint.configFile": "tslint.json",
  "tslint.exclude": "node_modules/**/*.*",
  "vetur.completion.tagCasing": "initial",
  "vetur.format.defaultFormatter.html": "prettier",
  "window.newWindowDimensions": "maximized",
  "window.title": "${rootName}${separator}${activeFolderMedium}/${activeEditorShort}",
  "workbench.colorTheme": "Atom One Dark",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.settings.editor": "json",
  "workbench.settings.useSplitJSON": true
}
